Tailorpus.controller('OtherCostCtrl', function($scope) {
  var count=0;
  $scope.inputList=[];
  $scope.toaddcost=function () {
    count++;
    $scope.inputList.push(count);
  }
  $scope.deleteStoEvent =function (index) {
    $scope.inputList.splice(index,1);
  }
});
