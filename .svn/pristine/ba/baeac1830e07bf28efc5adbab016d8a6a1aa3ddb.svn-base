.toggle.toggle-my-theme input:checked + .track {
  border-color: #0081C6;
  background-color: #0081C6;
}


.toggle-text > .track {

  width: auto;
  padding-left: 25px;
  padding-right: 25px;
  border: 0;
  height: 30px;
  background: #9e9e9e;
}
.AddQuote .toggle-text > .track,.material-price .toggle-text > .track{

  width: 72px !important;
  height:24px;
}
.AddQuote .item-toggle .toggle{
  top: 10px;
  right: 3px;
}
.material-price .item-toggle .toggle{
  top: 5px;
  right: 3px;
}
.toggle-text .handle {
  left: 6px;
  top:6px;
  width: 28px;
  height: 28px;
}
.AddQuote .toggle-text .handle,.material-price .toggle-text .handle {

  top:7px;
}
.AddQuote .toggle-text .handle,.material-price .toggle-text .handle {

  width: 20px;
  height: 20px;
}
.toggle-text .handle-text {
  font-size: 16px;
  color: white;
  position: absolute;
  top: 3px;

}
.AddQuote .toggle-text .handle-text,.material-price .toggle-text .handle-text{
  top: 1px;

}
.AddQuote .toggle-text .handle-text,.material-price .toggle-text .handle-text{
  font-size: 14px;
}
.toggle-text .handle-text-false,.toggle-text .handle-text-true{
  top: 3px;
}
.AddQuote .handle div:first-child,.material-price .handle div:first-child{
  left: -43px !important;
}
.AddQuote .handle div:last-child,.material-price .handle div:last-child{
  left: 22px !important;
}
.toggle-text .handle-text-false {
  left: 36px;
}

.toggle-text input:checked + .track .handle {
  -webkit-transform: none;
  -moz-transform: none;
  transform: none;
  left: calc(100% - 34px);
}
.AddQuote .toggle-text input:checked + .track .handle,.material-price .toggle-text input:checked + .track .handle {
  left: calc(100% - 26px);
  /*background: #0BB20C;*/
}
.track-text {
  line-height: 26px;
  color: white;
  font-size: 18px;
  padding-left: 10px;
  padding-right: 10px;
}
